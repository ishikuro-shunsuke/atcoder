#!/bin/sh

expect -c "
spawn acc login
expect username:
send ${ATCODER_USERNAME}\n
expect password:
send ${ATCODER_PASSWORD}\n'
expect eof
catch wait result
exit [ lindex $result 3 ]
" && \
expect -c "
spawn oj login https://atcoder.jp
expect Username:
send ${ATCODER_USERNAME}\n
expect Password:
send ${ATCODER_PASSWORD}\n
expect eof
catch wait result
exit [ lindex $result 3 ]
"
