#!/bin/sh

# acc
expect -c "
spawn acc login
expect username:
send ${ATCODER_USERNAME}\n
expect password:
send ${ATCODER_PASSWORD}\n'
expect OK
exit 0
"

# oj
expect -c "
spawn oj login https://atcoder.jp
expect Username:
send ${ATCODER_USERNAME}\n
expect Password:
send ${ATCODER_PASSWORD}\n
expect SUCCESS
exit 0
"
